<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.ListView?>
<AnchorPane prefHeight="377.0" prefWidth="462.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Menu.ControllerOfChartSetUpWindow">
   <children>
      <VBox alignment="CENTER" layoutX="12.0" layoutY="146.0" spacing="10.0">
         <children>
            <Label alignment="CENTER" contentDisplay="RIGHT" prefHeight="26.0" prefWidth="450.0" text="X-axis">
               <graphic>
                  <TextField fx:id="xAxisInputField" onKeyPressed="#search" />
               </graphic>
            </Label>

            <Label contentDisplay="RIGHT" text="Y-axis">
               <graphic>
                  <TextField fx:id="yAxisInputField" onKeyPressed="#search2" />
               </graphic>
            </Label>

            <Label fx:id="zAxisInputLabel" contentDisplay="RIGHT" text="Z-axis">
               <graphic>
                  <TextField fx:id="zAxisInputField" text="df" />
               </graphic>
            </Label>
         </children>
      </VBox>
      <HBox layoutX="170.0" layoutY="177.0">
         <children>
            <ListView fx:id="listView" prefHeight="80.0" maxWidth="180.0"/>
         </children>
      </HBox>
      <HBox layoutX="170.0" layoutY="220.0">
         <children>
            <ListView fx:id="listView2" prefHeight="80.0" maxWidth="180.0"/>
         </children>
      </HBox>
      <HBox layoutX="170.0" layoutY="263.0">
         <children>
            <ListView fx:id="listView3" prefHeight="80.0" maxWidth="180.0"/>
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutX="203.0" layoutY="55.0">
         <children>
            <ToggleButton fx:id="button2D" mnemonicParsing="false" onAction="#click2D" text="2D">
               <toggleGroup>
                  <ToggleGroup fx:id="toggleGroup2D3D" />
               </toggleGroup>
            </ToggleButton>
            <ToggleButton fx:id="button3D" mnemonicParsing="false" onAction="#click3D" text="3D" toggleGroup="$toggleGroup2D3D" />
         </children>
      </HBox>
      <HBox layoutX="197.0" layoutY="288.0">
         <children>
            <Button fx:id="createChartButton" alignment="CENTER" mnemonicParsing="false" onAction="#createChart" prefHeight="26.0" prefWidth="69.0" text="Graph it!" />
         </children>
      </HBox>
      <Button fx:id="backToMainMenuButton" layoutY="1.0" mnemonicParsing="false" onAction="#backToMainMenu" text="Back to Main Menu" />
   </children>
</AnchorPane>
